<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>Coderz.Json.Evaluation</RootNamespace>
    <Version>1.0.0</Version>
    <AssemblyName>Coderz.Json.Evaluation</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Authors>Kelly Birr</Authors>
    <PackageProjectUrl>https://github.com/kellybirr/json-evaluator</PackageProjectUrl>
    <Description>A parser and evaluator for jQuery QueryBuilder rules with Newtonsoft.Json</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

</Project>
